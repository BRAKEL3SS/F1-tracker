<%- include('../partials/header') %>
<table>
    <thead>
      <tr>
        <th>Team name</th>
        <th>Base of Operations</th>
        <th>First Driver</th>
        <th>Second Driver</th>
      </tr>
    </thead>
    <tbody>
      <% teams.forEach(function(team) { %>
        <tr>
          <td><%= team.name %></td>
          <td><%= team.location %></td>
          <td><%= team.drivers[0] %></td>
          <td><%= team.drivers[1] %></td>
          <td><a href="/teams/<%= team._id %>">DETAILS</a></td>
        </tr>
      <!-- Close the forEach using EJS here -->
      <% }) %>
    </tbody>
  </table>












<%- include('../partials/footer') %>